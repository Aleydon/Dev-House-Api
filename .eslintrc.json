{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-unused-vars": [0, { "argsIgnorePattern": "next" }]
    }
}
